<?xml version="1.0" encoding="UTF-8"?>
<page
	xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://jboss.com/products/seam/pages
		http://jboss.com/products/seam/pages-2.2.xsd"
	login-required="true"
	timeout="600000">
	<description>#{sys.cantonHome.instanceInfoStr()}录入 - 最后使用时间: #{currentTime}</description>
	<param
		name="cantonId"
		value="#{sys.cantonHome.objId}" />
	<param
		name="soId"
		value="#{sys.cantonHome.soId}" />
	<param
		name="notSuper"
		value="#{sys.cantonHome.notSuper}" />
	
	<action
		execute="#{sys.cantonHome.validateEntityFound}"
		on-postback="false"
		if="#{not empty cantonId}" />
	<begin-conversation
		if="#{identity.loggedIn}"
		join="true"
		flush-mode="manual" />
	<action
		execute="#{sys.cantonHome.wire}"
		on-postback="false" />
</page>
